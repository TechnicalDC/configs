!! COLOR SCHEME 
! special
*.foreground:   #ddd
*.background:   #242730
*.cursorColor:  #ddd

! black
*.color0:       #1b1918
*.color8:       #766e6b

! red
*.color1:       #f22c40
*.color9:       #f22c40

! green
*.color2:       #5ab738
*.color10:      #5ab738

! yellow
*.color3:       #d5911a
*.color11:      #d5911a

! blue
*.color4:       #407ee7
*.color12:      #407ee7

! magenta
*.color5:       #6666ea
*.color13:      #6666ea

! cyan
*.color6:       #00ad9c
*.color14:      #00ad9c

! white
*.color7:       #a8a19f
*.color15:      #f1efee
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgba
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

!! UXTERM CONFIGURATION
UXTerm*renderFont: true
UXTerm*faceName: xft:Fira Code Medium:style=Regular:pixelsize=12
UXTerm*faceSize: 10
UXTerm*utf8: 2
UXTerm*locale: true
UXTerm*savelines: 5120


!! XTERM CONFIGURATION
XTerm*renderFont: true
XTerm*faceName: xft:Fira Code Medium:style=Regular:pixelsize=12
XTerm*faceSize: 10
XTerm*utf8: 2
XTerm*locale: true
XTerm*savelines: 5120

!! URXVT CONFIGGURATION
!! URxvt Appearance
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 16
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 5120
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false

!! URvxt Font
URxvt.font: xft:Fira Code Medium:style=Regular:pixelsize=12
URxvt.boldFont: xft:Fira Code Medium:style=Bold:pixelsize:12


!!  Extensions
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,font-size
!URxvt.keysum.Control-equal: font-size:bigger
!URxvt.keysum.Control-minus: font-size:smaller
!URxvt.keysum.Control-0: font-size:reset
!URxvt.keysum.C-question: font-size:show
URxvt.font-size.step: 2
!URxvt.copyCommand: xclip -i -selection clipboard
!URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.C-S-c: perl:clipboard:copy
URxvt.keysym.C-S-v: perl:clipboard:paste
! URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
! URxvt.keysym.M-Escape: perl:keyboard-select:activate
! URxvt.keysym.M-s: perl:keyboard-select:search
! URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1
